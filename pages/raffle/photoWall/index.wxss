.photo_wall{
  display: flex;
  flex-direction: column;
  
}
.photo_wall_count{
  height: 100rpx;
  margin-top: 50rpx;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
}
.photo_wall_list{
  width: 690rpx;
  margin-left: 30rpx;
  display: flex;
  flex-direction: row;
  flex-wrap:wrap-reverse; 
}
.photo_wall_list_image{
  width: 69rpx;
  height: 69rpx;
  /* background-color: red; */
  margin-bottom: 10rpx;
}
.image{
  width: 69rpx;
  height: 69rpx;
  /* background-color: red; */
  margin-bottom: 10rpx;
}
.load_more{
  width: 100%;
  height: 110rpx;
  font-size: 28rpx;
  color: #0066CC;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  margin-bottom: 300rpx;
}




